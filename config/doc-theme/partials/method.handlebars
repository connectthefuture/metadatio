<a name="method_{{name}}"></a>
<div class="method item{{#if access}} {{access}}{{/if}}">
    {{#if final}}<code>final</code> {{/if}}{{#if returnType}}{{#crossLink returnType}}{{/crossLink}} {{/if}}<strong
        class="name">`{{name}}`</strong>( `{{paramsList}} ` ) {{#if access}}<em>`/* {{access}} method */`</em>{{/if}}
    <br>
    {{{methodDescription}}}
    {{#if params}}
        <h4>Parameters:</h4>
        <table>
            <thead>
            <tr>
                <th>Name</th>
                <th>Type</th>
                <th>Description</th>
            </tr>
            </thead>
            <tbody>
            {{#params}}
                <tr>
                    <td>
                        {{#if optional}}
                            `[{{name}}{{#if optdefault}}={{optdefault}}{{/if}}]`
                        {{else}}
                            `{{name}}`
                        {{/if}}
                    </td>
                    <td>
                        <strong>{{#crossLink type}}{{type}}{{/crossLink}}</strong>
                    </td>
                    <td>
                        {{{description}}}
                        {{#if multiple}}
                            <abbr title="Denotes that this argument can contain (n) number">(*..n)</abbr>
                        {{/if}}
                        {{#if props}}
                            <ul>
                                {{#props}}
                                    <li>`{{name}}`
                                        <strong>{{#crossLink type}}{{/crossLink}}</strong> {{{description}}}
                                        {{#if props}}
                                            <ul>
                                                {{#props}}
                                                    <li>`{{name}}`
                                                        <strong>{{#crossLink type}}{{/crossLink}}</strong> {{{description}}}
                                                    </li>
                                                {{/props}}
                                            </ul>
                                        {{/if}}
                                    </li>
                                {{/props}}
                            </ul>
                        {{/if}}
                    </td>
                </tr>
            {{/params}}

            </tbody>
        </table>
    {{/if}}
    {{#if return}}
        {{#return}}
            <br>
            <div class="returns">
                <strong>Returns:</strong> {{#if type}}{{#crossLink type}}{{/crossLink}} {{/if}}{{{description}}}</div>
        {{/return}}
    {{/if}}
    {{#if throws}}
        {{#throws}}
            <br>
            <div class="throws">
                <strong>Throws:</strong> {{#if type}}{{#crossLink type}}{{/crossLink}} {{/if}}{{{description}}}</div>
        {{/throws}}
    {{/if}}
    {{#if example}}
        <h5>Example</h5>
        {{{example}}}
    {{/if}}
</div>
